{
  "name": "mirasava",
  "version": "0.0.1",
  "description": "Boilerplate for react project.",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier --config ./.prettierrc --write srcClientAdmin/**/*.js",
    "csslint": "stylelint srcClientAdmin/universal/**/*.css",
    "test": "jest",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "lint": "eslint srcClientAdmin/",
    "lint:fix": "eslint --fix srcClientAdmin",
    "start:client": "NODE_PATH=./srcClientAdmin NODE_ENV=development babel-node --presets react ./srcClientAdmin/server/server.js",
    "start:backend": "NODE_ENV=localhost nodemon ./srcServer/src/bin/server.js --exec ./srcServer/node_modules/.bin/babel-node --presets env,flow",
    "start": "concurrently --kill-others \"npm run start:backend\" \"npm run start:client\"",
    "clear:build": "rm -Rf ./build",
    "clear:transpiled": "rm -Rf ./transpiled",
    "create:folder:build": "mkdir build",
    "create:folder:transpiled": "mkdir transpiled",
    "copy:assets": "cp -R ./public/. ./build/",
    "build:server": "NODE_ENV=production webpack --config ./webpack/server.babel.js",
    "build:client": "NODE_ENV=production webpack --config ./webpack/client.babel.js",
    "build": "npm run clear:build && npm run clear:transpiled && npm run create:folder:build && npm run create:folder:transpiled && npm run copy:assets && npm run build:client && npm run build:server",
    "compile:server:render:admin": "npx babel ./srcClientAdmin --out-dir ./transpiled",
    "production": "NODE_PATH=./transpiled NODE_ENV=production node ./transpiled/server/server.js",
    "start:production": "npm run build && npm run compile:server:render:admin && npm run production"
  },
  "keywords": [
    "react",
    "redux",
    "redux observable",
    "ramda",
    "rxjs",
    "ssr",
    "server-side-rendering",
    "react router",
    "async imports",
    "webpack 4",
    "postCss",
    "hot-module-reloading",
    "babel",
    "express",
    "flow",
    "mongoose",
    "jest",
    "jsonWebToken",
    "hmr",
    "code-splitting",
    "webpack",
    "webpack-dev-middleware",
    "webpack-hot-middleware"
  ],
  "author": "Aleksey Mukho <aleksey.mukho@gmail.com>",
  "license": "MIT",
  "jest": {
    "testMatch": [
      "**/?(*.)(spec|test).js"
    ],
    "modulePaths": [
      "./srcClientAdmin"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "srcServer"
    ],
    "moduleNameMapper": {
      ".(css|svg|jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "!node_modules/react-runtime"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest"
    ]
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-import-inspector": "^2.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "concurrently": "^3.5.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.79.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.0",
    "nodemon": "^1.17.2",
    "pleeease-filters": "^4.0.0",
    "postcss-calc": "^6.0.1",
    "postcss-color-function": "^4.0.1",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^7.0.0",
    "postcss-custom-selectors": "^4.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-media-minmax": "^3.0.0",
    "postcss-nested": "^3.0.0",
    "postcss-nesting": "^6.0.0",
    "postcss-selector-matches": "^3.0.1",
    "postcss-selector-not": "^3.0.1",
    "prettier": "1.17.0",
    "react-hot-loader": "^4.3.5",
    "style-loader": "^0.22.1",
    "stylelint": "^9.1.1",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-standard": "^18.1.0",
    "stylelint-prettier": "^1.0.6",
    "url-loader": "^1.1.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-hot-middleware": "^2.21.2"
  },
  "dependencies": {
    "express": "^4.16.3",
    "import-inspector": "^2.0.0",
    "ramda": "^0.25.0",
    "react": "^16.4.2",
    "react-cookie": "^3.0.4",
    "react-dom": "^16.3.2",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.4",
    "recharts": "^1.0.0-beta.10",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.2.2",
    "universal-cookie": "^3.0.4",
    "universal-cookie-express": "^3.0.4"
  }
}
